package learningjava;
import java.util.Random;


public class generaterandom {
    static String getAlphaNumericString(int n){
        String AlphaNumericString = "ABCDEFGHJKLMNOPQRSTUVWXYZ"+"0123456789";

        StringBuilder stringbuild = new StringBuilder(n);

        for (int i = 0; i < n; i++){
            int index = (int)(AlphaNumericString.length()*Math.random());

            stringbuild.append(AlphaNumericString.charAt(index));
        }
        return stringbuild.toString();
    }

    public static void main(String[] args) {
	// write your code here
        int n = 6;
        System.out.println(generaterandom.getAlphaNumericString(n));

    }
}
